{
    "name": "mario1515/laravel-eccomerce-cart",
    "description": "A Laravel Package that includes an Ecommerce Cart",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mario1515\\LaravelCart\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
    "laravel": {
            "providers": [
                "Mario1515\\LaravelCart\\LaravelCartServiceProvider"
            ],
            "aliases": {
                "Cart": "Mario1515\\LaravelCart\\Facades\\Cart"
            }
        }
    }
}
